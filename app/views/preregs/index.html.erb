<% @preregs.each do |prereg| %>
  <tr>
    <td><%= prereg.email %></td>
    <td><%= prereg.name %></td>
    <td><%= link_to 'Show', prereg %></td>
    <td><%= link_to 'Edit', edit_prereg_path(prereg) %></td>
    <td><%= link_to 'Destroy', prereg, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

